{
  "type": "object",
  "description": "ImageSource",
  "title": "IImageSource",
  "required": ["path", "coordinates"],
  "additionalProperties": false,
  "properties": {
    "path": {
      "type": "string",
      "readOnly": true,
      "description": "Path that points to an image"
    },
    "coordinates": {
      "type": "array",
      "readOnly": true,
      "items": {
        "type": "array",
        "items": {
          "type": "number"
        },
        "minItems": 2,
        "maxItems": 2
      },
      "minItems": 4,
      "maxItems": 4,
      "default": [],
      "description": "Corners of image specified in longitude, latitude pairs"
    },
    "interpolate": {
      "type": "boolean",
      "description": "Whether to interpolate between pixels or grid cells when overzooming",
      "default": false
    }
  }
}
